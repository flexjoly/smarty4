<?xml version="1.0" encoding="iso-8859-1"?>
<smarty>
    <configSection name="internalCompiler">
    <compilerClass scope="default" value="\Smarty\Compiler"/>
    <parserPeg>
        <trace>off</trace>
        <checkGenerator>off</checkGenerator>
        <compile_check>on</compile_check>
        <force_compile>off</force_compile>
        <generatorClass>Smarty\Parser\Peg\Generator\Core</generatorClass>
        <ruleFilePostfix>.peg.inc</ruleFilePostfix>
        <parserFilePostfix>Parser.php</parserFilePostfix>
        <parserClassPostfix>Parser</parserClassPostfix>
    </parserPeg>
    <pegGenerator>
        <compile_check>on</compile_check>
        <trace>off</trace>
        <force_compile>off</force_compile>
        <generatorClass>Smarty\Tool\Parser\Peg\InternalTool\ParserGenerator</generatorClass>
        <ruleFilePostfix>Tool/Parser/Peg/InternalTool/ParserGenerator.peg.inc</ruleFilePostfix>
        <parserFilePostfix>Tool/Parser/Peg/Generator.php</parserFilePostfix>
    </pegGenerator>
             <sourceConfig language="Smarty">
                 <directoryPath value="./Parser/Source/Language/Smarty/"/>
                 <sourceConfigXml value="./SourceConfig.xml"/>
                 <configSection value="internalSourceSmarty"/>
             </sourceConfig>
        <defaultNode>resource</defaultNode>
    </configSection>
</smarty>