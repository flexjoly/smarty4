    <pegparser name="Core">

        <token name="Id">
            <rule>/[a-zA-Z_\x7f-\xff][a-zA-Z0-9_\x7f-\xff]* /</rule>
         </token>

        <token name="Attr">
            <rule>/[\S]+/</rule>
         </token>

        <token name="OpenP">
            <attribute>matchall</attribute>
            <rule>/\s*\(\s* /</rule>
         </token>

        <token name="OpenB">
            <attribute>matchall</attribute>
            <rule>/\s*\[\s* /</rule>
         </token>

        <token name="OpenC">
            <attribute>matchall</attribute>
            <rule>/\{\s* /</rule>
         </token>

         <token name="CloseP">
            <attribute>matchall</attribute>
            <rule>/\s*\)\s* /</rule>
         </token>

        <token name="CloseB">
            <attribute>matchall</attribute>
            <rule>/\s*\}/</rule>
         </token>

        <token name="CloseC">
            <attribute>matchall</attribute>
            <rule>/\s*\}/</rule>
         </token>

        <token name="Equal">
            <attribute>matchall</attribute>
            <rule>/\s*=\s* /</rule>
         </token>

        <token name="Dollar">
            <attribute>matchall</attribute>
            <rule>/\$/</rule>
        </token>

        <token name="Hatch">
            <attribute>matchall</attribute>
            <rule>/#/</rule>
        </token>

        <token name="Comma">
            <attribute>matchall</attribute>
            <rule>/\s*,\s* /</rule>
        </token>

        <token name="Ptr">
            <attribute>matchall</attribute>
            <rule>/->/</rule>
        </token>
    </pegparser>

